<div class="col-sm-6 col-lg-3">
    <div class="card comp card-item">
        <a href='/video/<%= item.slug %>'>
            <img class="card-img-top" src=<%= item.image %> alt=<%= item.name %> />
        </a>
        <div class="card-body">
            <a href='/video/<%= item.slug %>'>
                <h5 class="card-title"><%= item.name %></h5>
            </a>
        </div>
    </div>
</div>
